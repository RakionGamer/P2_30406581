<div style="position: relative; width: 328px; left: 45px; top: -317px">
  <div class="form-group">
    <label for="exampleFormControlInput1" style="position: relative; top: -6.5px; left: 2px"><%= __('Email')%></label>
    <input required="true" type="email" class="form-control" id="exampleFormControlInput1"
      name="email"
      value="<%= get.email%>"
      placeholder="<%= __('IngreseEmail')%>"
    />
  </div>
  <div class="form-group">
    <label
      for="exampleFormControlInput2"
      style="position: relative; top: -6.5px; left: 2px"
      ><%= __('Name')%></label
    >
    <input
      required="true"
      type="text"
      class="form-control"
      id="exampleFormControlInput2"
      name="name"
      value="<%= get.name%>"
      placeholder="<%= __('IngreseNombre')%>"
    />
  </div>
  <label
    for="exampleFormControlTextarea1"
    style="position: relative; top: -6.5px; left: 2px"
    ><%= __('Comentario')%></label
  >
  <textarea
    required="true"
    class="form-control"
    id="exampleFormControlTextarea1"
    rows="3"
    name="commentary"
    placeholder="<%= __('IngreseComentario')%>"
  ></textarea
  ><%= get.commentary%>
  <br />
  <div class="form-group">
    <div class="g-recaptcha" data-sitekey="<%=KEY_PUBLIC%>"></div>
  </div>
  <button
    onclick="vf()"
    type="submit"
    class="btn btn-primary"
    style="margin-top: 0px; margin-bottom: 10px; margin-left: 34%"
  >
  <%= __('Send')%>
  </button>
  <input type="hidden" id="ipaddress" name="ipaddress" value="<%= get.ipaddress%>"/>
  <input type="hidden" name="date" value="<%= get.date%>" />
  <input type="hidden" name="hour" value="<%= get.hour%>" />
  <input type="hidden" id="country" name="country" value="<%= get.country%>" />
</div>
